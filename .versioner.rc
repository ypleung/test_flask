[versionner]
file = ./VERSION
date_format = %Y-%m-%d
up_part = patch
;default_init_version = 1.0.0

[vcs]
engine = git
commit_message = '%(version)s'
;tag_params =
;  -f
;  --local-user=some-key-id

[file:README.md]
enabled = false
search = ^\s*__version__\s*=.*$
replace = __version__ = '%(version)s'
date_format = %Y-%m-%d
match = line
search_flags = 
encoding = utf-8

